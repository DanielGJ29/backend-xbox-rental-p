{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "runtime": "V2",
    "numReplicas": 1,
    "startCommand": "sleep 10 && npm run start",
    //"healthcheckPath": "/",
    "sleepApplication": true,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  },
  //"env": {
  //  "ENABLE_ALPINE_PRIVATE_NETWORKING": true
 // }
}
